{
    "compileOnSave": true,
    "compilerOptions": {
        "target": "ES2015",
        "sourceRoot": "src",
        "outDir": "temp",
        "lib": ["ES2017", "dom"],
        "sourceMap": true,
        "jsx": "react",
        "jsxFactory": "h",
        "jsxFragmentFactory": "Fragment",
        "module": "ES6",
        "moduleResolution": "node",
        "noImplicitReturns": true,
        "strict": true,
        "suppressImplicitAnyIndexErrors": true,
        "noImplicitAny": false
    }
}
