{
    "compileOnSave": true,
    "compilerOptions": {
        "target": "es6",
        "outDir": "temp",
        "lib": [
            "ES2019",
            "dom"
        ],
        "sourceMap": true,
        "jsx": "react",
        "jsxFactory": "h",
        "jsxFragmentFactory": "Fragment",
        "module": "ES6",
        "moduleResolution": "node",
        "noImplicitReturns": true,
        "strict": true,
        "suppressImplicitAnyIndexErrors": true,
        "noImplicitAny": false,
        "baseUrl": ".",
    }
}